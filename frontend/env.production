# ========================================
# VARIABLES DE ENTORNO - FRONTEND PRODUCCIÓN
# ========================================
# Este archivo contiene las variables de entorno para el frontend en producción
# Se carga automáticamente cuando REACT_APP_ENV=production

# Configuración del entorno
REACT_APP_ENV=production

# URL de la API backend
REACT_APP_API_URL=http://localhost:3003/api

# Configuración adicional para producción
REACT_APP_DEBUG=false
REACT_APP_ENABLE_LOGS=false
